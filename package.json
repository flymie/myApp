{
  "name": "myApp",
  "version": "1.0.0",
  "private": true,
  "description": "mini",
  "templateInfo": {
    "name": "mobx",
    "typescript": true,
    "css": "less"
  },
  "scripts": {
    "build:weapp:prod": "cross-env NODE_ENV=production taro build --type weapp",
    "build:weapp": "cross-env NODE_ENV=development taro build --type weapp",
    "build:dd:prod": "cross-env NODE_ENV=production taro build --type dd",
    "build:dd": "cross-env NODE_ENV=development taro build --type dd",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "cross-env NODE_ENV=development taro build --type weapp --watch",
    "dev:dd": "cross-env NODE_ENV=development taro build --type dd --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/components": "3.3.4",
    "@tarojs/react": "3.3.4",
    "@tarojs/runtime": "3.3.4",
    "@tarojs/taro": "3.3.4",
    "mobx": "^4.8.0",
    "mobx-react": "^6.1.4",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "taro-plugin-tailwind": "^1.2.8"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@tarojs/mini-runner": "3.3.4",
    "@tarojs/webpack-runner": "3.3.4",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.13.6",
    "babel-preset-taro": "3.3.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.15.0",
    "eslint-config-taro": "3.3.4",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "stylelint": "9.3.0",
    "typescript": "^4.1.0"
  }
}
